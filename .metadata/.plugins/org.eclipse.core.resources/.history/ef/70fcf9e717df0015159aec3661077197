package model;

import epi.ServerInterface;

public class CreateProjectModel {
	
	private static CreateProjectModel instance = null;
	
	private CreateProjectModel(){
		
	}
	
	public static CreateProjectModel getInstance(){
		if(instance == null){
			instance = new CreateProjectModel();
		}
		return instance;
	}
	
	public void create_project(String name) throws Exception{
		ServerInterface.getInstance().get_stub().createProjectFolder(name);
	}

}
