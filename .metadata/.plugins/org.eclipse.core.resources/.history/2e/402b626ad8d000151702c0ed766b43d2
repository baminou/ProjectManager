package model;


public class PCRModel extends SpecificProjectModel {
	
	private String _label_protocol;
	private String _value_definition;
	private String _platform;
	private static PCRModel instance = null;
	
	protected PCRModel(){
		super();
	}
	
	public static PCRModel getInstance(){
		if(instance==null){
			instance = new PCRModel();
		}
		return instance;
	}

	public String get_platform() {
		return _platform;
	}

	public void set_platform(String _platform) {
		this._platform = _platform;
	}

	public String get_label_protocol() {
		return _label_protocol;
	}

	public void set_label_protocol(String _label_protocol) {
		this._label_protocol = _label_protocol;
	}

	public String get_value_definition() {
		return _value_definition;
	}

	public void set_value_definition(String _value_definition) {
		this._value_definition = _value_definition;
	}
	
	
}
