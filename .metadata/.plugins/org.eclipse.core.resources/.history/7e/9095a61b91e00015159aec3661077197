package model;

import app.Project;
import controller.SelectExistingProjectController;

public class GeneralInfoModel {
	private Project _project;

	public Project get_project() {
		return _project;
	}

	public void set_project(Project _project) {
		this._project = _project;
	}

	private static GeneralInfoModel instance = null;
	
	public static GeneralInfoModel getInstance(){
		if(instance == null){
			instance = new GeneralInfoModel();
		}
		return instance;
	}

	public int get_id() {
		return _id;
	}
	public void set_id(int _id) {
		this._id = _id;
	}
}
