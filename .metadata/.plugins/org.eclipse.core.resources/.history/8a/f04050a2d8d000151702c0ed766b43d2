package model;


public class SequencingModel extends SpecificProjectModel {
	
	private String _library_contruction;
	private String _library_strategy;
	private String _genome_build;
	private String _processed_data;
	
	private static SequencingModel instance = null;
	
	private SequencingModel(){
	}
	
	public static SequencingModel getInstance(){
		if(instance == null){
			instance = new SequencingModel();
		}
		return instance;
	}

	public SequencingModel(String library_construction,String library_strategy, String data_processing, String genome_build, String processed_data) {
		_library_contruction = library_construction;
		_library_strategy = library_strategy;
		_genome_build = genome_build;
		_processed_data = processed_data;
	}

	public String get_library_contruction() {
		return _library_contruction;
	}

	public void set_library_contruction(String _library_contruction) {
		this._library_contruction = _library_contruction;
	}

	public String get_library_strategy() {
		return _library_strategy;
	}

	public void set_library_strategy(String _library_strategy) {
		this._library_strategy = _library_strategy;
	}
	public String get_genome_build() {
		return _genome_build;
	}

	public void set_genome_build(String _genome_build) {
		this._genome_build = _genome_build;
	}

	public String get_processed_data() {
		return _processed_data;
	}

	public void set_processed_data(String _processed_data) {
		this._processed_data = _processed_data;
	}

}
